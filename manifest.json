{
  "manifest_version": 3,
  "name": "Crypto Wallet Extension",
  "version": "1.0.0",
  "description": "A Chrome extension for managing your crypto wallet.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
